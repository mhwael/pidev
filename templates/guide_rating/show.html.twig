{% extends 'back.html.twig' %}

{% block title %}GuideRating{% endblock %}

{% block body %}
    <h1>GuideRating</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ guide_rating.id }}</td>
            </tr>
            <tr>
                <th>RatingValue</th>
                <td>{{ guide_rating.ratingValue }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ guide_rating.comment }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
