            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_guide_rating_index') }}">back to list</a>

    <a href="{{ path('app_guide_rating_edit', {'id': guide_rating.id}) }}">edit</a>

    {{ include('guide_rating/_delete_form.html.twig') }}
